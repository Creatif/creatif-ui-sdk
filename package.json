{
    "name": "creatif-api-sdk",
    "version": "0.0.1",
    "description": "API SDK for Creatif",
    "license": "MIT",
    "repository": {
        "url": "git@github.com:Creatif/creatif-ui-sdk.git"
    },
    "main": "build/index.js",
    "keywords": [
        "timer",
        "interval"
    ],
    "_moduleAliases": {
        "@lib": "/build/lib/*",
        "@app": "/build/app/*",
        "@root": "/build/*"
    },
    "author": {
        "email": "marioskrlec222@gmail.com",
        "name": "Mario Å krlec"
    },
    "scripts": {
        "dev": "vite",
        "build-dev": "vite build --mode development",
        "test-build": "tsc main.ts && node main.js",
        "build-prod": "vite build --mode production",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
        "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
        "compile": "tsc",
        "preview": "vite preview"
    },
    "files": [
        "build",
        "index.d.ts",
        "types.d.ts"
    ],
    "devDependencies": {
        "@babel/preset-env": "7.22.7",
        "@babel/preset-react": "7.22.5",
        "@babel/preset-typescript": "7.22.5",
        "@rollup/plugin-commonjs": "25.0.2",
        "@testing-library/dom": "9.3.1",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@types/chai": "^4.3.5",
        "@types/jest": "29.5.3",
        "@types/module-alias": "^2.0.2",
        "@types/node": "^20.5.7",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "6.0.0",
        "@typescript-eslint/parser": "6.0.0",
        "@vitejs/plugin-react": "^4.0.4",
        "chai": "^4.3.7",
        "eslint": "8.44.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-react": "7.32.2",
        "jest": "29.6.1",
        "module-alias": "2.2.3",
        "prettier": "3.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-test-renderer": "18.2.0",
        "rollup": "3.26.2",
        "rollup-plugin-typescript2": "0.35.0",
        "ts-jest": "29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "5.1.6",
        "uuid": "9.0.0",
        "vite": "^4.4.9",
        "vite-plugin-dts": "^3.5.3"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "antd": "^5.8.5",
        "axios": "^1.4.0",
        "framer-motion": "^10.16.1",
        "react-query": "^3.39.3"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
