{
    "name": "creatif-ui-sdk",
    "version": "0.0.34",
    "description": "UI SDK to create Creatif CMS",
    "license": "MIT",
    "type": "module",
    "repository": {
        "url": "git@github.com:Creatif/creatif-ui-sdk.git"
    },
    "main": "build/index.js",
    "keywords": [
        "cms",
        "creatif"
    ],
    "author": {
        "email": "marioskrlec222@proton.me",
